{
    "workbench.colorTheme": "Solarized Custom Dark",
    "workbench.iconTheme": "vscode-icons",
    "editor.minimap.maxColumn": 70,
    "editor.minimap.showSlider": "always",
    "editor.minimap.side": "left",
    "editor.minimap.size": "fill",
    "security.workspace.trust.untrustedFiles": "open",
    "gitlens.advanced.messages": {
        "suppressGitDisabledWarning": true
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "explorer.incrementalNaming": "smart",
    "explorer.fileNesting.enabled": true,
    "editor.mouseWheelZoom": true,
    "explorer.autoReveal": false,
    "colorHelper.pickerForm": "simple",

    
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.matchBrackets":"always",
    "bracket-pair-colorizer-2.showHorizontalScopeLine": true,
    "bracket-pair-colorizer-2.forceUniqueOpeningColor":true,
    "bracket-pair-colorizer-2.highlightActiveScope": true,
    "bracket-pair-colorizer-2.showBracketsInGutter": true,
    "bracket-pair-colorizer-2.colors": [
        "Gold",
        "Orchid",
        "LightSkyBlue"
    ],
    "bracket-pair-colorizer-2.forceIterationColorCycle": true,
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "bracket-pair-colorizer-2.activeScopeCSS": [
        "borderStyle : solid",
        "borderWidth : 1px",
        "borderColor : {color}",
        "opacity: 0.5"
    ],
    "files.autoSave": "afterDelay",
    "editor.guides.bracketPairsHorizontal": "active",
    "editor.guides.highlightActiveIndentation": true,
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#FFD700",
        "editorBracketHighlight.foreground2": "#DA70D6",
        "editorBracketHighlight.foreground3": "#179fff",
        "editorBracketPairGuide.background1": "#ffd90080",
        "editorBracketPairGuide.background2": "#CC66CC80",
        "editorBracketPairGuide.background3": "#87CEFA80",
        "editorBracketPairGuide.activeBackground1": "#ffd90080",
        "editorBracketPairGuide.activeBackground2": "#CC66CC80",
        "editorBracketPairGuide.activeBackground3": "#87CEFA80"
    },
    "php.validate.executablePath": "",
    "editor.fontSize": 16,
    "npm.keybindingsChangedWarningShown": true,
    "editor.fontFamily": "Consolas,FagoEx,Century Gothic, 'Courier New',monospace",
    "[php]": {
        "editor.defaultFormatter": "kokororin.vscode-phpfmt"
    },
    "editor.formatOnSave": true,
    "[php]": {
        "editor.formatOnSave": true
    },
    "phpfmt.detect_indent": true,
    "json.schemas": [
    
    
    ],
    "editor.cursorBlinking": "expand",
    "terminal.integrated.cursorBlinking": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "pdf-preview.default.cursor": "select",
}
